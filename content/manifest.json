{
  "name": "Pinry",
  "version": "0.0.2",
  "description": "Pins images from current page to a Pinry installation.",
  "manifest_version": 2,
  "icons": {
    "48": "ico-48.png",
    "128": "ico-128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "ico-128.png",
    "default_title": "Pinry"
  },
  "background": {
    "scripts": [ "background.js" ]
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "chrome_style": true
  }
}
